(declare-const p00 Bool) 
(declare-const p01 Bool) 
(declare-const p02 Bool)
(declare-const p10 Bool) 
(declare-const p11 Bool) 
(declare-const p12 Bool)
(declare-const p20 Bool) 
(declare-const p21 Bool) 
(declare-const p22 Bool)
(declare-const p30 Bool) 
(declare-const p31 Bool) 
(declare-const p32 Bool)
(declare-const p40 Bool) 
(declare-const p41 Bool) 
(declare-const p42 Bool)
; each node has exactly 1 color
(assert ((_ pbeq 1 1 1 1) p00 p01 p02))
(assert ((_ pbeq 1 1 1 1) p10 p11 p12))
(assert ((_ pbeq 1 1 1 1) p20 p21 p22))
(assert ((_ pbeq 1 1 1 1) p30 p31 p32))
(assert ((_ pbeq 1 1 1 1) p40 p41 p42))
; no neighbors share color
;Nodes 0 1 2 are a clique, so none can share a color
(assert ((_ at-most 1) p00 p10 p20))
(assert ((_ at-most 1) p01 p11 p21))
(assert ((_ at-most 1) p02 p12 p22))
;Nodes 1 2 3 are a clique, so none can share a color
(assert ((_ at-most 1) p10 p20 p30))
(assert ((_ at-most 1) p11 p21 p31))
(assert ((_ at-most 1) p12 p22 p32))
;Nodes 1 2 4 are a clique, so none can share a color
(assert ((_ at-most 1) p10 p20 p40))
(assert ((_ at-most 1) p11 p21 p41))
(assert ((_ at-most 1) p12 p22 p42))
(check-sat)
